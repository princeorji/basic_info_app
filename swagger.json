{
    "swagger": "2.0",
    "info": {
        "title": "BASIC INFO APP",
        "description": "An API that retrieves basic information",
        "version": "1.0.0"
    },
    "paths": {
        "/": {
            "get": {
                "summary": "Returns user Information",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "email": {
                                            "type": "string",
                                            "example": "princemikeorji@gmail.com"
                                        },
                                        "current_datetime": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2025-01-30T09:30:00Z"
                                        },
                                        "github_url": {
                                            "type": "string",
                                            "format": "url",
                                            "example": "https://github.com/princeorji/basic_info_app"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}